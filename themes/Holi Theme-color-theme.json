{
	"name": "Holi Theme",
	"type": "dark",
	"colors": {
		"foreground": "#85a4c0",
		"widget.shadow": "#011322",
		"focusBorder": "#0f2a3f",
		"selection.background": "#4772b8",

		// INPUT
		"input.background": "#031b2e",
		"input.border": "#85a4c0",
		"input.foreground": "#eeefff",
		"input.placeholderForeground": "#eeefff94",
		// INPUT -- ENDS

		// DROP DOWN
		"dropdown.background": "#030314",
		"dropdown.border": "#85a4c0",
		"dropdown.foreground": "#eeefffe7",
		"dropdown.listBackground": "#030314",
		"settings.dropdownBackground": "#030314",
		"settings.dropdownForeground": "#eeefffe7",
		"settings.dropdownBorder": "#85a4c0",
		// DROP DOWN -- ENDS

		// ACTIVITY BAR
		"activityBar.background": "#030314d7",
		"activityBar.foreground": "#85a4c0",
		"activityBar.activeBackground": "#010d1a",
		"activityBar.activeBorder": "#85a4c0",
		"activityBar.activeFocusBorder": "#5fabee",
		"activityBarBadge.background": "#1c789c",
		// ACTIVITY BAR -- ENDS

		// SIDE BAR
		"sideBar.background": "#030314d7",
		"sideBar.foreground": "#85a4c0",
		"sideBarSectionHeader.background": "#030314e3",
		"sideBarSectionHeader.border": "#85a5c041",
		"sideBar.border": "#030314d7",
		//SIDE BAR -- ENDS

		// STATUS BAR
		"statusBar.background": "#040424bd",
		"statusBar.foreground": "#d0e4e4",
		"statusBarItem.activeBackground": "#072c4d",
		"statusBarItem.hoverBackground": "#052038",
		// STATUS BAR -- ENDS

		// MENU 
		"menu.background": "#030314",
		// MENU -- ENDS

		// SCROLLBAR
		"scrollbar.shadow": "#010b14",
		"scrollbarSlider.activeBackground": "#084d8180",
		"scrollbarSlider.background": "#084d8180",
		"scrollbarSlider.hoverBackground": "#084d8180",
		// SCROLLBAR - ENDS

		// TITLEBAR
		"titleBar.activeBackground": "#030314",
		"titleBar.inactiveBackground": "#030314ce",
		"titleBar.activeForeground": "#eeefff",
		"titleBar.inactiveForeground": "#eeefffb0",
		// TITLEBAR -- ENDS
		
		// EDITOR GROUP
		"editorGroupHeader.tabsBackground": "#030314",
		"editorGroupHeader.tabsBorder": "#012a4a",
		"editorGroup.border": "#012a4a",
		"editorGroupHeader.border": "#012a4a",
		// EDITOR GROUP -- ENDS
		
		// TAB
		"tab.activeBackground": "#0e3250",
		"tab.activeForeground": "#d2dee7",
		"tab.border": "#272B3B",
		"tab.activeBorder": "#262A39",
		"tab.unfocusedActiveBorder": "#262A39",
		"tab.inactiveBackground": "#01121f",
		"tab.inactiveForeground": "#6e8ca5",
		"tab.unfocusedActiveForeground": "#5f7e97",
		"tab.unfocusedInactiveForeground": "#5f7e97",
		// TAB -- ENDS

		// BREADCRUMB
		"breadcrumb.background": "#030314",
		"breadcrumb.foreground": "#998ddb",
		"breadcrumb.activeSelectionForeground": "#eeefff",
		"breadcrumb.focusForeground": "#eeefff",
		"breadcrumbPicker.background": "#030314da",
		// BREADCRUMB -- ENDS

		// EDITOR
		"editor.background": "#030314",
		"editor.foreground": "#eeefff", 
		"editor.selectionBackground": "#1d3b54",
		"editorLineNumber.foreground": "#4b6481",
    	"editorLineNumber.activeForeground": "#C5E4FD",
		"editorWidget.background": "#030314",
		"editorWidget.border": "#6a50b1",
		// EDITOR -- ENDS

		"sideBarTitle.foreground": "#bbbbbb",
		"window.activeBorder": "#012a4a",
		"window.inactiveBorder": "#033458"
	},
	"tokenColors": [
		{
			"name": "Comment",
			"scope": [
				"comment",
				"punctuation.definition.comment"
			],
			"settings": {
				"fontStyle": "italic",
				"foreground": "#446e69"
			}
		},
		{
			"name": "Variables",
			"scope": [
				"variable",
				"string constant.other.placeholder"
			],
			"settings": {
				"foreground": "#d6e7ff"
			}
		},
		{
			"name": "Colors",
			"scope": [
				"constant.other.color"
			],
			"settings": {
				"foreground": "#ffffff"
			}
		},
		{
			"name": "Invalid",
			"scope": [
				"invalid",
				"invalid.illegal"
			],
			"settings": {
				"foreground": "#FF5370"
			}
		},
		{
			"name": "Keyword, Storage",
			"scope": [
				"keyword",
				"storage.type",
				"storage.modifier"
			],
			"settings": {
				"foreground": "#0fe468"
			}
		},
		{
			"name": "Operator, Misc",
			"scope": [
				"keyword.control",
				"constant.other.color",
				"punctuation",
				"meta.tag",
				"punctuation.definition.tag",
				"punctuation.separator.inheritance.php",
				"punctuation.definition.tag.html",
				"punctuation.definition.tag.begin.html",
				"punctuation.definition.tag.end.html",
				"punctuation.section.embedded",
				"keyword.other.template",
				"keyword.other.substitution"
			],
			"settings": {
				"foreground": "#d6b007"
			}
		},
		{
			"name": "Tag",
			"scope": [
				"entity.name.tag",
				"meta.tag.sgml",
				"markup.deleted.git_gutter"
			],
			"settings": {
				"foreground": "#ec8e01"
			}
		},
		{
			"name": "Function, Special Method",
			"scope": [
				"entity.name.function",
				"meta.function-call",
				"variable.function",
				"support.function",
				"keyword.other.special-method"
			],
			"settings": {
				"foreground": "#78f3e9"
			}
		},
		{
			"name": "Block Level Variables",
			"scope": [
				"meta.block variable.other"
			],
			"settings": {
				"foreground": "#977cca"
			}
		},
		{
			"name": "Other Variable, String Link",
			"scope": [
				"support.other.variable",
				"string.other.link"
			],
			"settings": {
				"foreground": "#f07178"
			}
		},
		{
			"name": "Number, Constant, Function Argument, Tag Attribute, Embedded",
			"scope": [
				"constant.numeric",
				"constant.language",
				"support.constant",
				"constant.character",
				"constant.escape",
				"variable.parameter",
				"keyword.other.unit",
				"keyword.other"
			],
			"settings": {
				"foreground": "#f83db0"
			}
		},
		{
			"name": "String, Symbols, Inherited Class, Markup Heading",
			"scope": [
				"string",
				"constant.other.symbol",
				"constant.other.key",
				"entity.other.inherited-class",
				"markup.heading",
				"markup.inserted.git_gutter",
				"meta.group.braces.curly constant.other.object.key.js string.unquoted.label.js"
			],
			"settings": {
				"foreground": "#49c6ec"
			}
		},
		{
			"name": "Class, Support",
			"scope": [
				"entity.name",
				"support.type",
				"support.class",
				"support.orther.namespace.use.php",
				"meta.use.php",
				"support.other.namespace.php",
				"markup.changed.git_gutter",
				"support.type.sys-types"
			],
			"settings": {
				"foreground": "#e94f71"
			}
		},
		{
			"name": "Entity Types",
			"scope": [
				"support.type"
			],
			"settings": {
				"foreground": "#B2CCD6"
			}
		},
